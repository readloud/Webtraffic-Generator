<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <stroke android:width="1.0dip" android:color="#ffee463b" />
            <gradient android:startColor="#a1f7f7f7" android:endColor="#a1fcfcfc" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="5.0dip" />
            <stroke android:width="0.0dip" android:color="#6649abbd" />
            <solid android:color="#6649abbd" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <stroke android:width="1.0dip" android:color="#ffee463b" />
            <gradient android:startColor="#00000000" android:endColor="#00000000" android:angle="-90.0" />
        </shape>
    </item>
</selector>